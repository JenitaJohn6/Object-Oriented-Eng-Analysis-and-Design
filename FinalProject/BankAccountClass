package coe528.project;
/**
 *
 * @author jjmar
 */
public class BankAccount {
    
    String accountNumber;
    Customer customer;
     
    public BankAccount(Customer c,String n){   
        accountNumber = n;
        customer =c;
    }
    @Override
    public String toString(){
        return ("AccountNumber:"+ accountNumber + "\n" + customer);
    }
}
